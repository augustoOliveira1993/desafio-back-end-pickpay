    "scripts": {
        "start": "nodemon ./src/index.js",
        "model:generate": "npx sequelize-cli model:generate --name",
        "migrate:generate": "npx sequelize-cli migration:generate --name",
        "seed:generate": "npx sequelize-cli seed:generate --name",
        "seed": "npx sequelize-cli db:seed",
        "seed:all": "npx sequelize-cli db:seed:all",
        "seed:undo": "npx sequelize-cli db:seed:undo",
        "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:down": "npx sequelize-cli db:migrate:undo"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "helmet": "^7.0.0",
        "mysql2": "^3.6.0",
        "sequelize": "^6.32.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "dotenv": "^16.3.1",
        "express-validator": "^7.0.1",
        "nodemon": "^3.0.1",
        "sequelize-cli": "^6.6.1"
    }
}
